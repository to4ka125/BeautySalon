<UserControl x:Class="BeautySalon.View.BackupRecovery1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BeautySalon.View"
             mc:Ignorable="d" 
         Height="360" Width="500" Background="#fff">
    <Grid>
        <TextBlock Text="Резервное восстановление базы данных" FontSize="20" Margin="10,10,10,0" Grid.ColumnSpan="2"/>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="BtnStruct" Style="{StaticResource BtnStruct}" Width="300" Height="80" Content="Резервное востановление БД" Click="BtnStruct_Click" VerticalAlignment="Top"/>
        </StackPanel>

    </Grid>
</UserControl>
